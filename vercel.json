{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.ignivio.co"
        }
      ],
      "destination": "https://ignivio.co/:splat",
      "permanent": true
    }
  ]
}